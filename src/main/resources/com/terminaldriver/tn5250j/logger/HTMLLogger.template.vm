<html>
<head>
<style>
.console {
    background-color: black;
    color:green;
}
.consoleline pre{
	margin:0;
}
.consoleCell{
	padding-bottom:1em;
}
.reverseGreenText{
        color:black;
	background-color:green;
}
.reverseWhiteText{
        color:black;
	background-color:white;
}
.reverseBlueText{
        color:black;
	background-color:blue;
}

.reverseTurquoiseText{
        color:black;
	background-color:turquoise;
}

.reverseYellowText{
        color:black;
	background-color:yellow;
}

.reverseRedText{
        color:black;
	background-color:red;
}

.reversePinkText{
        color:black;
	background-color:pink;
}

.whiteText{
	color:white;
}
.blueText{
	color:SteelBlue;
}

.turquoiseText{
	color:turquoise;
}

.yellowText{
	color:yellow;
}

.redText{
	color:red;
}

.pinkText{
	color:pink;
}
.underline{
        text-decoration: underline;
}
.table {
    display:table;
}
.row {
    display:table-row;
}
.cell {
    display:table-cell;
}

</style>
</head>

<div class="table">
	#foreach( $info1 in $info )
	<div class="row consoleRow">
		<div class="cell consoleCell">
             $info1.getScreenHtml()
		</div>
		<div class="cell">
			$info1.getLogText()
		</div>
	</div>
    #end
</div>

</html>